{
  "name": "annict",
  "description": "Annict API client library",
  "version": "0.0.5",
  "author": "nukosuke",
  "browser": "lib/annict.js",
  "bugs": {
    "url": "https://github.com/nukosuke/annict.js/issues"
  },
  "dependencies": {
    "es6-promise": "^3.3.1",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.2.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.0",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^0.8.2",
    "tslint": "^3.12.1",
    "typedoc": "^0.4.5",
    "typescript": "^2.0.3",
    "typings": "^1.3.3",
    "webpack": "^1.13.2"
  },
  "homepage": "https://github.com/nukosuke/annict.js#readme",
  "keywords": [
    "anime",
    "api",
    "library",
    "service"
  ],
  "license": "MIT",
  "main": "lib/annict.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nukosuke/annict.js.git"
  },
  "scripts": {
    "build": "npm run build:main && npm run build:browser",
    "build:browser": "$(npm bin)/webpack -p",
    "build:main": "$(npm bin)/tsc -p tsconfig.json",
    "build:tsd": "$(npm bin)/tsc -p tsconfig.json -d",
    "docs": "$(npm bin)/typedoc --mode file --readme none --name 'annict.js リファレンス' --target ES5 --out ./docs",
    "lint": "$(npm bin)/tslint -c tslint.json './src/**/*.ts'",
    "prepublish": "npm run build",
    "test": "npm run test:main && npm run test:browser",
    "test:browser": "$(npm bin)/karma start",
    "test:main": "$(npm bin)/mocha './test/**/*.spec.js' -r ./test/globals --recursive"
  },
  "typings": "lib/annict.d.ts"
}
