{
  "name": "annict",
  "description": "Annict API client library",
  "version": "0.0.8",
  "author": "nukosuke",
  "browser": "./lib/annict.js",
  "bugs": {
    "url": "https://github.com/nukosuke/annict.js/issues"
  },
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/qs": "^6.2.30",
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.2.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "chai": "^4.1.2",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-webpack": "^2.0.2",
    "mocha": "^5.0.0",
    "phantomjs-prebuilt": "^2.1.12",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^3.4.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.10.0",
    "typescript": "^2.0.3",
    "webpack": "^3.10.0"
  },
  "homepage": "https://github.com/nukosuke/annict.js#readme",
  "keywords": [
    "anime",
    "api",
    "library",
    "service"
  ],
  "license": "MIT",
  "main": "./lib/annict.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nukosuke/annict.js.git"
  },
  "scripts": {
    "build": "npm run build:main && npm run build:browser",
    "build:browser": "$(npm bin)/webpack -p",
    "build:main": "$(npm bin)/tsc -p tsconfig.json",
    "build:tsd": "$(npm bin)/tsc -p tsconfig.json -d",
    "docs": "$(npm bin)/typedoc --mode file --readme none --target ES5 --out ./docs",
    "lint": "$(npm bin)/tslint -c tslint.json './src/**/*.ts'",
    "prepublish": "npm run build",
    "test": "npm run test:main && npm run test:browser",
    "test:browser": "$(npm bin)/karma start",
    "test:main": "$(npm bin)/mocha './test/**/*.spec.js' -r ./test/globals --recursive"
  },
  "types": "./lib/annict.d.ts"
}
