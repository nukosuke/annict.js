{
  "name": "annict",
  "version": "0.0.4",
  "description": "Annict API client library",
  "main": "lib/annict.js",
  "browser": "lib/annict.js",
  "typings": "lib/annict.d.ts",
  "scripts": {
    "test": "npm run test:main && npm run test:browser",
    "test:main": "./node_modules/.bin/mocha './test/**/*.spec.js' -r ./test/globals --recursive",
    "test:browser": "./node_modules/.bin/karma start",
    "build": "npm run build:main && npm run build:browser",
    "build:main": "./node_modules/.bin/tsc -p tsconfig.json",
    "build:tsd": "./node_modules/.bin/tsc -p tsconfig.json -d",
    "build:browser": "./node_modules/.bin/webpack -p",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nukosuke/annict.js.git"
  },
  "keywords": [
    "anime",
    "api",
    "library",
    "service"
  ],
  "author": "nukosuke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nukosuke/annict.js/issues"
  },
  "homepage": "https://github.com/nukosuke/annict.js#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.12",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    "typings": "^1.3.3",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "es6-promise": "^3.3.1",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.2.1"
  }
}
